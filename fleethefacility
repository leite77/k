    info:SetDesc("Credits To The Owner: leite\nDiscord Name For Contact: 86x1\nPing: "..math.floor(game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValue() or 0).."ms.\nServer Stuff:\nSize: "..#game:GetService("Players"):GetPlayers().."/"..game:GetService("Players").MaxPlayers..".\nBeast Power: "..rep.CurrentPower.Value)
end)
